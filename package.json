{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "heroku-postbuild": "NODE_ENV=production yarn run build:all",
    "heroku-run": "NODE_ENV=production yarn run server:run",
    "build-and-run-like-prod": "yarn run heroku-postbuild && yarn run heroku-run",
    "build:all": "webpack --progress --config webpack/webpack.server.config.ts && webpack --progress --config webpack/webpack.frontend.config.ts",
    "server:compile": "webpack  --progress --config webpack/webpack.server.config.ts",
    "server:compile-w": "webpack --watch  --progress --config webpack/webpack.server.config.ts",
    "server:run": "node dist/bundle-server.js",
    "server:run-w": "nodemon dist/bundle-server.js --watch dist"
  },
  "engines": {
    "node": "9.5.0",
    "yarn": "1.9.4"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.37",
    "@types/jquery": "^3.2.12",
    "@types/knex": "^0.14.21",
    "@types/lodash": "^4.14.74",
    "@types/node": "^10.9.2",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/request": "^2.0.3",
    "@types/webpack": "^4.4.11",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.0",
    "@types/webpack-node-externals": "^1.6.3",
    "awesome-typescript-loader": "^5.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-alpha.6",
    "css-loader": "^1.0.0",
    "express": "^4.15.4",
    "google-auth-library": "^1.6.1",
    "jquery": "^3.2.1",
    "knex": "^0.15.2",
    "lodash": "^4.17.4",
    "moment": "^2.21.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "pg": "^7.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.9",
    "style-loader": "^0.22.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-node-externals": "^1.6.0"
  }
}

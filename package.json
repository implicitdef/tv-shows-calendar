{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "heroku-postbuild": "npm run build:all",
    "heroku-run": "npm run server:run",
    "build:all":
      "webpack --progress --config webpack/webpack.server.config.ts && webpack --progress --config webpack/webpack.frontend.config.ts",
    "server:compile":
      "webpack  --progress --config webpack/webpack.server.config.ts",
    "server:compile-w":
      "webpack --watch  --progress --config webpack/webpack.server.config.ts",
    "server:run": "node dist/bundle-server.js",
    "server:run-w": "nodemon dist/bundle-server.js --watch dist"
  },
  "engines": {
    "node": "8.3.0",
    "npm": "5.3.0"
  },
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jquery": "^3.2.12",
    "@types/knex": "0.0.61",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.30",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.9",
    "@types/request": "^2.0.3",
    "@types/webpack": "^3.0.11",
    "@types/webpack-dev-middleware": "^1.9.3",
    "@types/webpack-hot-middleware": "^2.16.0",
    "awesome-typescript-loader": "^3.2.3",
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "^4.0.0-alpha.6",
    "css-loader": "^0.28.7",
    "express": "^4.15.4",
    "google-auth-library": "^0.11.0",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "moment": "^2.21.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "pg": "^7.3.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "source-map-support": "^0.4.18",
    "style-loader": "^0.18.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-node-externals": "^1.6.0"
  }
}

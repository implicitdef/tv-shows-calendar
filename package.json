{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "heroku-postbuild": "NODE_ENV=production yarn run build:all",
    "heroku-run": "NODE_ENV=production yarn run server:run",
    "build-and-run-like-prod": "yarn run heroku-postbuild && yarn run heroku-run",
    "build:all": "webpack --progress --config webpack/webpack.server.config.ts && webpack --progress --config webpack/webpack.frontend.config.ts",
    "server:compile": "webpack  --progress --config webpack/webpack.server.config.ts",
    "server:compile-w": "webpack --watch  --progress --config webpack/webpack.server.config.ts",
    "server:run": "node dist/bundle-server.js",
    "server:run-w": "nodemon dist/bundle-server.js --watch dist"
  },
  "engines": {
    "node": "10.1.0",
    "yarn": "1.12.3"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.3.1",
    "@martin_hotell/rex-tils": "^2.1.1",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.16.1",
    "@types/jquery": "^3.3.21",
    "@types/knex": "^0.15.1",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "@types/request": "^2.0.3",
    "@types/webpack": "^4.4.24",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.0",
    "@types/webpack-node-externals": "^1.6.3",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.3",
    "css-loader": "^2.1.0",
    "express": "^4.16.4",
    "google-auth-library": "^3.1.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.10",
    "pg": "^7.8.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "redux-react-hook": "^3.1.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.10",
    "style-loader": "^0.23.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-react": "^3.2.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.6.0"
  }
}

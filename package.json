{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "heroku-postbuild": "NODE_ENV=production yarn run build:all",
    "heroku-run": "NODE_ENV=production yarn run server:run",
    "build-and-run-like-prod": "yarn run heroku-postbuild && yarn run heroku-run",
    "build:all": "webpack --progress --config webpack/webpack.server.config.ts && webpack --progress --config webpack/webpack.frontend.config.ts",
    "server:compile": "webpack  --progress --config webpack/webpack.server.config.ts",
    "server:compile-w": "webpack --watch  --progress --config webpack/webpack.server.config.ts",
    "server:run": "node dist/bundle-server.js",
    "server:run-w": "nodemon dist/bundle-server.js --watch dist"
  },
  "engines": {
    "node": "10.1.0",
    "yarn": "1.9.4"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.37",
    "@types/jquery": "^3.3.21",
    "@types/knex": "^0.14.26",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/redux-actions": "^2.3.1",
    "@types/request": "^2.0.3",
    "@types/webpack": "^4.4.17",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.0",
    "@types/webpack-node-externals": "^1.6.3",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.3",
    "css-loader": "^1.0.0",
    "express": "^4.16.4",
    "google-auth-library": "^2.0.0",
    "jquery": "^3.2.1",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "moment": "^2.21.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.4",
    "pg": "^7.5.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.6",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.9",
    "style-loader": "^0.23.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^3.1.3",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.6.0"
  }
}
